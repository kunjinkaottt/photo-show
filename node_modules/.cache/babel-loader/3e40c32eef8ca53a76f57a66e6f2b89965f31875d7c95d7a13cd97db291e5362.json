sible')\r\n    },\r\n\r\n  },\r\n  created() {\r\n\r\n  },\r\n  mounted() {\r\n    var back = document.getElementsByClassName('back')\r\n    if (this.$store.state.visible) {\r\n      back[0].style.visibility = 'visible'  //vuex的state.visible为真，使弹窗可视\r\n      // document.documentElement.style.overflow = 'hidden'; //隐藏滚动条\r\n      this.winX = window.scrollX;\r\n      this.winY = window.scrollY;\r\n\r\n    } else {\r\n      back[0].style.visibility = 'hidden'//vuex的state.visible为假，隐藏弹窗\r\n      // document.documentElement.style.overflow = 'auto'; //如果 垂直 或 水平方向有溢出，显示滚动条。如果是scroll\r\n      this.winX = null;\r\n      this.winY = null;\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.back {\r\n  z-index: 2;\r\n  visibility: hidden;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, .5);\r\n\r\n  .dialog {\r\n    z-index: 3;\r\n    visibility: inherit; //跟随父级可视化\r\n    position: fixed; //固定定位，弹窗永远弹出在可视窗口的中间\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background-color: rgb(255, 255, 255);\r\n    width: 28%;\r\n    height: 45%;\r\n    border-radius: 20px;\r\n\r\n    .logo {\r\n      width: 20%;\r\n      // position: absolute;\r\n      // left: 50%;\r\n      // margin-left: -50%;\r\n\r\n    }\r\n  }\r\n\r\n\r\n}\r\n</style>"],"mappings":"AAgBA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA,GAEA;EACAC,KAAA;IACA,sBAAAC,CAAAC,MAAA,EAAAC,MAAA;MACA;MACA,IAAAC,IAAA,GAAAC,QAAA,CAAAC,sBAAA;MACA,IAAAJ,MAAA;QACAE,IAAA,IAAAG,KAAA,CAAAC,UAAA;QACA;QACA;QACA,KAAAX,IAAA,GAAAY,MAAA,CAAAC,OAAA;QACA,KAAAZ,IAAA,GAAAW,MAAA,CAAAE,OAAA;QAEAF,MAAA,CAAAG,gBAAA;UACA,IAAAf,IAAA,aAAAC,IAAA;YACAW,MAAA,CAAAI,QAAA,CAAAhB,IAAA,EAAAC,IAAA;UACA;QACA;MAEA;QACAM,IAAA,IAAAG,KAAA,CAAAC,UAAA;QACA;QACA;QACA,KAAAX,IAAA;QACA,KAAAC,IAAA;QAGAW,MAAA,CAAAG,gBAAA;UACA,IAAAf,IAAA,aAAAC,IAAA;YACAW,MAAA,CAAAI,QAAA,CAAAhB,IAAA,EAAAC,IAAA;UACA;QACA;MACA;IACA;EACA;EACAgB,QA