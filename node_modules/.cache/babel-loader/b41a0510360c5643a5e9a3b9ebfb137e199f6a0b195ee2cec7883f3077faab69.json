ocument.documentElement.style.overflow = 'hidden'; //隐藏滚动条\r\n    } else {\r\n      back[0].style.visibility = 'hidden'//vuex的state.visible为假，隐藏弹窗\r\n      // document.documentElement.style.overflow = 'auto'; //如果 垂直 或 水平方向有溢出，显示滚动条。如果是scroll\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.back {\r\n  z-index: 2;\r\n  visibility: hidden;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, .5);\r\n\r\n  .dialog {\r\n    z-index: 3;\r\n    visibility: inherit; //跟随父级可视化\r\n    position: fixed; //固定定位，弹窗永远弹出在可视窗口的中间\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background-color: rgb(255, 255, 255);\r\n    width: 28%;\r\n    min-width: 462px;//浏览器拉到最小的时候\r\n    height: 45%;\r\n    border-radius: 20px;\r\n\r\n    .logo {\r\n      width: 30%;\r\n      // position: absolute;\r\n      // left: 50%;\r\n      // margin-left: -50%;\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AAoBA;AACA,IAAAA,IAAA;AACA,IAAAC,IAAA;AAEAC,MAAA,CAAAC,gBAAA;EACA,IAAAH,IAAA,aAAAC,IAAA;IACAC,MAAA,CAAAE,QAAA,CAAAJ,IAAA,EAAAC,IAAA;EACA;AACA;AAEA,SAAAI,oBAAA;EACAL,IAAA,GAAAE,MAAA,CAAAI,OAAA;EACAL,IAAA,GAAAC,MAAA,CAAAK,OAAA;AACA;AAEA,SAAAC,mBAAA;EACAR,IAAA;EACAC,IAAA;AACA;AAEA;EACAQ,IAAA;EACAC,KAAA;IACA,QACA;EACA;EACAC,QAAA,GAEA;EACAC,KAAA;IACA,sBAAAC,CAAAC,MAAA,EAAAC,MAAA;MACA;MACA,IAAAC,IAAA,GAAAC,QAAA,CAAAC,sBAAA;MACA,IAAAJ,MAAA;QACAE,IAAA,IAAAG,KAAA,CAAAC,UAAA;QACA;QACAf,mBAAA;MACA;QACAW,IAAA,IAAAG,KAAA,CAAAC,UAAA;QACA;QACAZ,kBAAA;MACA;IACA;EACA;;EACAa,QAAA;EACAC,OAAA;IACAC,YAAA;MACA,KAAAC,MAAA,CAAAC,MAAA;IACA;EACA;EACAC,QAAA;IACA,IAAAV,IAAA,GAAAC,QAAA,CAAAC,sBAAA;IACA,SAAAM,MAAA,CAAAG,KAAA,CAAAC,OAAA;MACAZ,IAAA,IAAAG,KAAA,CAAAC,UAAA;MACA;IACA;MACAJ,IAAA,IAAAG,KAAA,CAAAC,UAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}